    "axios": "^1.7.7",
    "compression": "^1.7.4",
    "copy-to-clipboard": "^3.3.1",
    "express": "^4.19.2",
    "highlight.js": "^11.10.0",
    "react": "^18.2.0",
    "react-accessible-treeview": "^2.9.1",
    "react-ace": "^12.0.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.3.0",
    "react-markdown": "^9.0.3",
    "react-router-dom": "^6.14.1",
    "react-simple-code-editor": "^0.14.1",
